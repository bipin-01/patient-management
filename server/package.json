{
  "name": "patient-management",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start:dev": "nodemon",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "transpile": "tsc",
    "clean": "rimraf dist",
    "build": "yarn clean && yarn transpile",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "knex": "knex --knexfile=src/knexFile.ts",
    "seed": "yarn knex seed:run --verbose",
    "migrate": "yarn knex migrate:latest --verbose",
    "rollback": "yarn knex migrate:rollback --verbose",
    "make:seeder": "yarn knex seed:make -x ts --env development",
    "make:migration": "yarn knex migrate:make -x ts --env development",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --list-different",
    "prettier:format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.14",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "fastify": "^4.6.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@sentry/node": "^7.14.2",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mongoose": "^5.11.97",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "camelize": "^1.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "helmet": "^6.0.0",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^6.7.2",
    "mysql2": "^2.3.3",
    "to-snake-case": "^1.0.0",
    "winston": "^3.8.2"
  }
}
